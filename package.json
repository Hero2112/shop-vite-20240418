{
  "name": "shop-vite",
  "version": "8.3.0",
  "private": true,
  "author": "chuzhixin",
  "scripts": {
    "dev": "set VITE_CJS_IGNORE_WARNING=true && vite",
    "dev:mac": "export VITE_CJS_IGNORE_WARNING=true && vite",
    "dev:vue-tsc": "set VITE_CJS_IGNORE_WARNING=true && vue-tsc --noEmit && vite",
    "dev:https": "set VITE_CJS_IGNORE_WARNING=true && vite --config ./vite.config.dev.ts",
    "vue-tsc": "vue-tsc --noEmit",
    "build": "set VITE_CJS_IGNORE_WARNING=true && vue-tsc --noEmit && vite build",
    "build:mac": "export VITE_CJS_IGNORE_WARNING=true && vue-tsc --noEmit && vite build",
    "build:fast": "set VITE_CJS_IGNORE_WARNING=true && set NODE_OPTIONS=--max-old-space-size=8192 && vite build",
    "build:fast:mac": "export VITE_CJS_IGNORE_WARNING=true && vite build",
    "build:website": "set VITE_CJS_IGNORE_WARNING=true && set NODE_OPTIONS=--max-old-space-size=8192 && vite build --config ./vite.config.website.ts && node write.version.js",
    "build:website:mac": "export VITE_CJS_IGNORE_WARNING=true && vite build --config ./vite.config.website.ts && node write.version.js",
    "build:report": "set VITE_CJS_IGNORE_WARNING=true && set NODE_OPTIONS=--max-old-space-size=8192 && vite build --config ./vite.config.dev.ts",
    "build:vue-shop-vite": "set VITE_CJS_IGNORE_WARNING=true && vite build",
    "preview": " set VITE_CJS_IGNORE_WARNING=true && vite preview",
    "lint:eslint": "eslint . --fix",
    "lint:prettier": "prettier {src,mock,library,types}/**/*.{html,vue,css,sass,scss,js,ts} --write",
    "lint:stylelint": "stylelint {src,mock,library}/**/*.{vue,css,sass,scss} --fix --cache --cache-location node_modules/.cache/stylelint/",
    "global:install": "cnpm i -g nrm cnpm npm-check-updates pnpm",
    "globle:update": "ncu -g",
    "module:install": "pnpm install",
    "module:update": "ncu -u --reject vite-plugin-mock,eslint --registry https://registry.npmmirror.com && npm run module:install",
    "module:reinstall": "rimraf node_modules && npm run module:install",
    "git": "start ./git.sh",
    "generate-pwa-assets": "pwa-assets-generator",
    "template": "plop"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@imengyu/vue3-context-menu": "^1.4.0",
    "@kangc/v-md-editor": "^2.3.15",
    "@logicflow/core": "^1.2.26",
    "@logicflow/extension": "^1.2.26",
    "@lucky-canvas/vue": "^0.1.11",
    "@opentiny/vue": "^3.15.0",
    "@vueuse/core": "^10.9.0",
    "@vueuse/head": "^2.0.0",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "axios": "^1.6.8",
    "dayjs": "^1.11.10",
    "echarts": "^5.5.0",
    "element-plus": "^2.7.0",
    "jsencrypt": "^3.3.2",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "mockjs": "^1.1.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "qs": "^6.12.1",
    "typeit": "^8.8.3",
    "vsv-icon": "^1.1.9",
    "vue": "^3.4.23",
    "vue-draggable-plus": "^0.4.0",
    "vue-echarts": "^6.6.9",
    "vue-i18n": "^9.12.1",
    "vue-json-viewer": "^3.0.4",
    "vue-pdf-embed": "^2.0.3",
    "vue-qr": "^4.0.9",
    "vue-router": "^4.3.1",
    "vue3-gantt": "1.1.8-7",
    "vue3-puzzle-vcode": "^1.1.7",
    "xgplayer": "^3.0.16",
    "xgplayer-hls.js": "^3.0.16"
  },
  "devDependencies": {
    "@element-plus/eslint-config": "^0.0.20240416",
    "@types/lodash-es": "^4.17.12",
    "@types/mockjs": "^1.0.10",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.15",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/compiler-sfc": "^3.4.23",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "autoprefixer": "^10.4.19",
    "chokidar": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.25.0",
    "lint-staged": "^15.2.2",
    "picocolors": "^1.0.0",
    "plop": "^4.0.1",
    "postcss-html": "^1.6.0",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.75.0",
    "stylelint": "^16.3.1",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "terser": "^5.30.3",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.9",
    "vite-plugin-banner": "^0.7.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "2.9.8",
    "vite-plugin-pwa": "^0.19.8",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-unplugin": "^1.7.0",
    "vite-plugin-vitebar": "^0.0.5",
    "vue-global-api": "^0.4.1",
    "vue-tsc": "^2.0.13"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "homepage": "https://vue-admin-beautiful.com/shop-vite",
  "license": "Mozilla Public License Version 2.0",
  "lint-staged": {
    "*.{js,ts,vue}": [
      "npm run lint",
      "npm run lint:prettier",
      "git add"
    ]
  },
  "participants": [
    "FlowPeakFish"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-admin-beautiful/shop-vite.git"
  }
